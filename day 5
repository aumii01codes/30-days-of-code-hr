//Day 5: Loops

import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) throws IOException {
        Scanner read = new Scanner(System.in);
        int n = read.nextInt();
        read.close();
        for(int i=1; i<=10; i++){
            System.out.println(n + " x " + i + " = " + n*i);
        }
    }
}